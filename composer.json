{
    "name": "katod/htmlmin",
    "description": "HTMLMinImproved is fork of HTMLMin - Simple HTML Minifier For Laravel 5, but with improved minification rules for blade files(no space between tags anymore)",
    "keywords": ["laravel", "framework", "html minifier", "html", "minifier", "HTMLMinImproved", "Laravel HTMLMinImproved", "Laravel-HTMLMin-Improved"],
    "license": "MIT",
    "authors": [
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Raza Mehdi",
            "email": "srmk@outlook.com"
        },
        {
            "name": "kat0d9",
            "email": "kat0d999@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/contracts": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/filesystem": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/http": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/routing": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "illuminate/view": "5.1.*|5.2.*|5.3.*|5.4.*|5.5.*|5.6.*|5.7.*",
        "mrclay/minify": "^2.2"
    },
    "require-dev": {
        "graham-campbell/testbench": "^3.1|^4.0|^5.0",
        "mockery/mockery": "^0.9.4|^1.0",
        "phpunit/phpunit": "^4.8|^5.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Katod\\HTMLMin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Katod\\Tests\\HTMLMin\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.0-dev"
        },
        "laravel": {
          "providers": [
            "Katod\\HTMLMin\\HTMLMinServiceProvider"
        ],
          "aliases": {
            "HTMLMin": "Katod\\HTMLMin\\Facades\\HTMLMin"
        }
    }
},
"minimum-stability": "dev",
"prefer-stable": true
}
